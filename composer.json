{
    "name": "maksa988/laravel-wayforpay",
    "description": "WayForPay payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.5",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\WayForPay\\Facades\\": "src/Facades",
            "Maksa988\\WayForPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maksa988\\WayForPay\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maksa988\\WayForPay\\WayForPayServiceProvider"
            ],
            "aliases": {
                "WayForPay": "Maksa988\\WayForPay\\Facades\\WayForPay"
            }
        }
    }
}
